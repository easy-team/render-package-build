{
  "name": "render-package-build",
  "version": "0.1.0",
  "description": "render service package build script",
  "keywords": [
    "npm",
    "npm package",
    "unit test",
    "eslint",
    "travis",
    "travis-ci",
    "code coverage",
    "changelog"
  ],
  "dependencies": {
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "codecov": "^3.0.0",
    "conventional-changelog-cli": "^1.3.5",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-eslint": "^4.0.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "nyc --reporter=html --reporter=text mocha --timeout=1000000",
    "cov": "nyc report --reporter=lcov && codecov --token=e971d1e7-39e2-44b1-b1ee-f4345b0adbee",
    "ci": "npm run lint && npm run cov",
    "ii": "npm install --registry https://registry.npm.taobao.org"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "test/*.test.js"
    ]
  },
  "ci": {
    "version": "8, 9"
  },
  "author": "hubcarl@126.com",
  "license": "MIT"
}
